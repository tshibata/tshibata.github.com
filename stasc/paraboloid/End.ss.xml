<g
 xmlns="http://www.w3.org/2000/svg"
 xmlns:xlink="http://www.w3.org/1999/xlink"
 xmlns:ss="http://tshibata.github.io/2013/stasc">
	<text x="160" y="240" text-anchor="middle" font-size="28" font-family="Helvetica" fill="white">
		Game Over
	</text>
	<text x="160" y="260" text-anchor="end" font-size="14" font-family="Helvetica" fill="white">
		All Time Best:
	</text>
	<text x="160" y="260" text-anchor="start" font-size="14" font-family="Helvetica" fill="white">
		<ss:property name="AlltimeBest" target="textContent"/>
	</text>
	<text x="160" y="280" text-anchor="end" font-size="14" font-family="Helvetica" fill="white">
		Today's Best:
	</text>
	<text x="160" y="280" text-anchor="start" font-size="14" font-family="Helvetica" fill="white">
		<ss:property name="TodaysBest" target="textContent"/>
	</text>
	<text x="160" y="300" text-anchor="middle" font-size="14" font-family="Helvetica" fill="White" text-decoration="underline" style="cursor:pointer;" onclick="start();">
		Replay
	</text>
	<a xlink:href="help.html">
		<text x="160" y="320" text-anchor="middle" font-size="14" font-family="Helvetica" fill="white" text-decoration="underline">
			Help
		</text>
	</a>
</g>

